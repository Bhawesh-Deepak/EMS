@model IEnumerable<EMS.Core.Entities.Survey.MonitoringDetails>
<table id="monitorDetailstbl" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>S.No</th>
            <th>Agency Name </th>
            <th>Percentage</th>
        </tr>
    </thead>
    <tbody>

        @{
            int count = 1;

            foreach (var data in Model)
            {
                <tr>
                    <td>
                        @count
                    </td>
                    <td>
                        @data.AgancyName
                    </td>
                    <td>
                        @data.Percentage
                    </td>
                </tr>
                count++;
            }

        }



    </tbody>

</table>

<script>
    $(document).ready(function () {
        $("#monitorDetailstbl").DataTable({
            scrollX: true
        })

        setTimeout(function () {
            $(".sorting").trigger('click')
        }, 300)
    });
</script>