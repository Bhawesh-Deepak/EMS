@model IEnumerable<EMS.Core.Entities.Survey.Options>


<div class="box">
    <div class="box-header with-border">
        <h3 class="box-title">User List</h3>
    </div>
    <div class="box-body">
        <table id="questionTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Option</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var data in Model)
                {
                    <tr>
                        <td>
                            @data.OptionValue
                        </td>
                    </tr>
                }
            </tbody>

        </table>

        <br />
        <br />

        <button type="button" class="btn btn-danger modalButton pull-right" data-dismiss="modal">
            Close
        </button>
    </div>

</div>